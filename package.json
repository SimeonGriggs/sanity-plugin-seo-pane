  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/sanity-plugin-seo-pane.git"
  },
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "@sanity/ui": "^0.37.2",
    "jed": "^1.1.1",
    "prop-types": "15.8.1",
    "react-query": "^3.34.8",
    "yoastseo": "^1.91.0"
  },
  "devDependencies": {
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-sanity": "5.1.0",
